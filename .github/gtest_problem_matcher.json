{
  "problemMatcher": [
    {
      "owner": "gtest-problem-matcher",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):\\s+(Failure)$",
          "file": 1,
          "line": 2,
          "severity": 3
        },
        {
          "regexp": "^(?!\\[).*$",
          "message": 1,
          "loop": true
        }
      ]
    }
  ]
}
